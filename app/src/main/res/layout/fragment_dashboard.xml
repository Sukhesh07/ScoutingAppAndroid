            android:textColor="@color/FEDS_WHITE"
            android:textSize="40sp"
            app:layout_constraintEnd_toStartOf="@+id/button_minus7"
            app:layout_constraintStart_toEndOf="@+id/button_plus7"
            app:layout_constraintTop_toBottomOf="@+id/tally6" />

        <TextView
            android:id="@+id/tally8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13sp"
            android:fontFamily="@font/faxsansbeta"
            android:text="0"
            android:textColor="@color/FEDS_WHITE"
            android:textSize="40sp"
            app:layout_constraintEnd_toStartOf="@+id/button_minus8"
            app:layout_constraintStart_toEndOf="@+id/button_plus8"
            app:layout_constraintTop_toBottomOf="@+id/tally7" />

        <CheckBox
            android:id="@+id/leavesTarmac"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="280sp"
            android:fontFamily="@font/faxsansbeta"
            android:textColor="@color/FEDS_WHITE"
            android:textSize="32sp"
            android:text="Leaves Tarmac?"
            android:buttonTint="@color/FEDS_WHITE"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/title_Autonomous" />

        <CheckBox
            android:id="@+id/humanScored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10sp"
            android:buttonTint="@color/FEDS_WHITE"
            android:fontFamily="@font/faxsansbeta"
            android:text="Human Scored?"
            android:textColor="@color/FEDS_WHITE"
            android:textSize="32sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/leavesTarmac" />

        <CheckBox
            android:id="@+id/playedDefence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="280sp"
            android:buttonTint="@color/FEDS_WHITE"
            android:fontFamily="@font/faxsansbeta"
            android:text="Played Defence?"
            android:textColor="@color/FEDS_WHITE"
            android:textSize="32sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider4" />

        <Chronometer
            android:id="@+id/stopwatch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10sp"
            android:fontFamily="@font/faxsansbeta"
            android:format="00:00"
            android:textColor="@color/FEDS_WHITE"
            android:textSize="42sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider7" />

        <ImageButton
            android:id="@+id/play_button"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_marginRight="70sp"
            android:background="@drawable/white_background"
            android:src="@drawable/play_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/stopwatch" />

        <ImageButton
            android:id="@+id/reset_button"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_marginLeft="70sp"
            android:background="@drawable/white_background"
            android:src="@drawable/refresh_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/stopwatch" />

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="200sp"
            android:layout_height="wrap_content"
            android:max="4"
            android:scaleX="1.5"
            android:scaleY="1.5"
            android:progressBackgroundTint="@color/FEDS_WHITE"
            android:thumbTint="@color/FEDS_WHITE"
            android:secondaryProgressTint="@color/FEDS_WHITE"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv" />

        <TextView
            android:id="@+id/tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bar Climbed: "
            android:layout_marginTop="80sp"
            android:fontFamily="@font/faxsansbeta"
            android:textColor="@color/FEDS_WHITE"
            android:textSize="32sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/stopwatch" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="300sp"
            android:layout_height="wrap_content"
            android:textColor="@color/FEDS_WHITE"
            android:layout_marginTop="20sp"
            android:inputType="textPersonName"
            android:text="Notes:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/seekBar" />

        <TextView
            android:id="@+id/title_rating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Overall Rating"
            android:fontFamily="@font/faxsansbeta"
            android:layout_marginTop="40sp"
            android:textSize="32sp"
            android:textColor="@color/FEDS_WHITE"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

        <RatingBar
            android:id="@+id/ratingBar2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:progressTint="@color/FEDS_WHITE"
            android:secondaryProgressTint="@color/FEDS_WHITE"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/title_rating" />

        <ImageView
            android:id="@+id/iv_output"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="250sp"
            android:adjustViewBounds="true"
            android:scaleType="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dashboard_title" />

        <Button
            android:id="@+id/finish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1350dp"
            android:backgroundTint="@color/FEDS_WHITE"
            android:text="Finish"
            android:textColor="@color/FEDS_BLACK"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dashboard_title" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>